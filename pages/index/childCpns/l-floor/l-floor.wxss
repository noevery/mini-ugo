.floor .hd {
    padding: 10rpx 0;
}
.floor .bd {
    overflow: hidden;
}
.floor .bd navigator {
    float: left;
    width: 33.3%;
}
.floor navigator:nth-last-child(-n+4) {
    height: 27.72711207vw;
    border-left: 10rpx solid #fff;
}
.floor navigator:nth-child(2),
.floor navigator:nth-child(3) {
    border-bottom: 10rpx solid #fff;
}
.floor navigator image {
    width: 100%;
    height: 100%;
}